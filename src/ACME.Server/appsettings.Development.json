{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AcmeServer": {
    "HostedWorkers": {
      "EnableValidationService": true,
      "EnableIssuanceService": true,

      "ValidationCheckInterval": 60,
      "IssuanceCheckInterval": 60
    },

    "_WebsiteUrl": "https://my-custom.site",

    "TOS": {
      "RequireAgreement": false,

      "_Url": "https://my-custom.tos",
      "_LastUpdate": "2020-01-23"
    }
  },

  "FileStore": {
    "NoncePath": "C:\\Temp\\Acme-Server\\Store\\Nonces",
    "AccountPath": "C:\\Temp\\Acme-Server\\Store\\Accounts",
    "WorkingPath": "C:\\Temp\\Acme-Server\\Store\\_work"
  }
}
